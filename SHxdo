local RVelocity, YVelocity = nil, 0.1
function sendnotifi(message)
 
 
 game.StarterGui:SetCore("SendNotification", {
     Title =  'ShadoWare';
     Text = message;
     Icon = 'rbxassetid://13209787743';
     Duration = "2";
 })
